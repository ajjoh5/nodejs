{{#contentFor "header-scripts"}}

    <!-- Dashboard JS -->
    <script src="/js/dashboard/app-particles.js"></script>

{{/contentFor}}

<div ng-app="appParticles" ng-controller="particlesController">
    <h1>Example App: Home</h1>

    <p>{{ group }}</p>

    <input type="text" ng-model="user.email" />
    <input type="text" ng-model="user.password" />
    <a href="#" ng-click="login()">Login</a>
    <a href="#" ng-click="logout()">Logout</a>

    <p><pre>{[{ data | json }]}</pre></p>
    <p><pre>{[{ particles | json }]}</pre></p>

    <a href="#" ng-click="addParticle()">Add Particle</a>

</div>
